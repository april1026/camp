#include <iostream>
#include <vector>

using namespace std;

int main() {
	int t;
	cin >> t;
	for (int k = 0; k < t; k++) {
		vector <char> s;
		string input;
		cin >> input;
//		cout << input << " " << input.size() << endl;
		bool infor = true;
		for (int i = 0; i < input.size() && infor; i++) {
			switch (input[i]){
				case '(':
				case '[':
//					cout << "([ " << i << endl;
					s.push_back(input[i]);
					break;
				case ')':
					if (s.size() > 0 && s.back() == '(') {
						s.pop_back();
//						cout << " ) " << i << endl;
						break;
					} else {
						cout << "No" << endl;
//						cout << ")else " << i << " i" << endl;
						infor = false;
						break;
					}
				case ']':
					if (s.size() > 0 && s.back() == '[') {
						s.pop_back();
//						cout << "] " << i << endl;
						break;
					} else {
						cout << "No" << endl;
//						cout << "]else " << i << endl;
						infor = false;
						break;
					}
			}
		}
		if (s.size() != 0 && infor) {
			cout << "No" << endl;
		} else if (s.size() == 0 && infor) {
			cout << "Yes" << endl;
		}
	}
}
