#include <iostream>
#include <vector>

using namespace std;

int main() {
	int t;
	cin >> t;
	vector <int> s;
	for (int k = 0; k < t; k++) {
		int input;
		cin >> input;
		switch (input){
			case 1:
				s.pop_back();
				break;
			case 2:
				cout << s.back() << endl;
				break;
			case 3:
				int x;
				cin >> x;
				s.push_back(x);
				break;
		}
	}
}
